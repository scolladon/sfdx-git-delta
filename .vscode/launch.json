{
	"version": "1.0.0",
	"configurations": [
	  {
		"type": "node",
		"request": "launch",
		"name": "full: sgd:source:delta",
		"program": "${workspaceFolder}/bin/run",
		"cwd": "${input:sfdxProjectDirectoryPath}",
		"args": [
		  "sgd:source:delta",
		  "-t",
		  "${input:sgdSourceDeltaTo}",
		  "-f",
		  "${input:sgdSourceDeltaFrom || HEAD}",
		  "-r",
		  "${input:sgdSourceDeltaRepo}",
		  "-i",
		  "${input:sgdSourceDeltaIgnore}",
		  "-D",
		  "${input:sgdSourceDeltaIgnoreDestructive}",
		  "-s",
		  "${input:sgdSourceDeltaSource}",
		  "-o",
		  "${input:sgdSourceDeltaOutput}",
		  "-a",
		  "${input:sgdSourceDeltaApiVersion}",
		  "-n",
		  "${input:sgdSourceDeltaInclude}",
		  "-N",
		  "${input:sgdSourceDeltaIncludeDestructive}"
		]
	  },
	  {
		"type": "node",
		"request": "launch",
		"name": "full: sgd:source:delta -d",
		"program": "${workspaceFolder}/bin/run",
		"cwd": "${input:sfdxProjectDirectoryPath}",
		"args": [
		  "sgd:source:delta",
		  "-t",
		  "${input:sgdSourceDeltaTo}",
		  "-f",
		  "${input:sgdSourceDeltaFrom || HEAD}",
		  "-r",
		  "${input:sgdSourceDeltaRepo}",
		  "-i",
		  "${input:sgdSourceDeltaIgnore}",
		  "-D",
		  "${input:sgdSourceDeltaIgnoreDestructive}",
		  "-s",
		  "${input:sgdSourceDeltaSource}",
		  "-o",
		  "${input:sgdSourceDeltaOutput}",
		  "-a",
		  "${input:sgdSourceDeltaApiVersion}",
		  "-n",
		  "${input:sgdSourceDeltaInclude}",
		  "-N",
		  "${input:sgdSourceDeltaIncludeDestructive}",
		  "-d"
		]
	  },
	  {
		"type": "node",
		"request": "launch",
		"name": "full: sgd:source:delta -W",
		"program": "${workspaceFolder}/bin/run",
		"cwd": "${input:sfdxProjectDirectoryPath}",
		"args": [
		  "sgd:source:delta",
		  "-t",
		  "${input:sgdSourceDeltaTo}",
		  "-f",
		  "${input:sgdSourceDeltaFrom || HEAD}",
		  "-r",
		  "${input:sgdSourceDeltaRepo}",
		  "-i",
		  "${input:sgdSourceDeltaIgnore}",
		  "-D",
		  "${input:sgdSourceDeltaIgnoreDestructive}",
		  "-s",
		  "${input:sgdSourceDeltaSource}",
		  "-o",
		  "${input:sgdSourceDeltaOutput}",
		  "-a",
		  "${input:sgdSourceDeltaApiVersion}",
		  "-n",
		  "${input:sgdSourceDeltaInclude}",
		  "-N",
		  "${input:sgdSourceDeltaIncludeDestructive}",
		  "-W"
		]
	  },
	  {
		"type": "node",
		"request": "launch",
		"name": "full: sgd:source:delta -d -W",
		"program": "${workspaceFolder}/bin/run",
		"cwd": "${input:sfdxProjectDirectoryPath}",
		"args": [
		  "sgd:source:delta",
		  "-t",
		  "${input:sgdSourceDeltaTo}",
		  "-f",
		  "${input:sgdSourceDeltaFrom || HEAD}",
		  "-r",
		  "${input:sgdSourceDeltaRepo}",
		  "-i",
		  "${input:sgdSourceDeltaIgnore}",
		  "-D",
		  "${input:sgdSourceDeltaIgnoreDestructive}",
		  "-s",
		  "${input:sgdSourceDeltaSource}",
		  "-o",
		  "${input:sgdSourceDeltaOutput}",
		  "-a",
		  "${input:sgdSourceDeltaApiVersion}",
		  "-n",
		  "${input:sgdSourceDeltaInclude}",
		  "-N",
		  "${input:sgdSourceDeltaIncludeDestructive}",
		  "-d",
		  "-W"
		]
	  },
	  {
		"type": "node",
		"request": "launch",
		"name": "simple: sgd:source:delta",
		"program": "${workspaceFolder}/bin/run",
		"cwd": "${input:sfdxProjectDirectoryPath}",
		"args": [
		  "sgd:source:delta",
		  "-f",
		  "${input:sgdSourceDeltaFrom}"
		]
	  },
	  {
		"type": "node",
		"request": "launch",
		"name": "simple: sgd:source:delta -d",
		"program": "${workspaceFolder}/bin/run",
		"cwd": "${input:sfdxProjectDirectoryPath}",
		"args": [
		  "sgd:source:delta",
		  "-f",
		  "${input:sgdSourceDeltaFrom}",
		  "-d",
		]
	  },
	  {
		"type": "node",
		"request": "launch",
		"name": "simple: sgd:source:delta -W",
		"program": "${workspaceFolder}/bin/run",
		"cwd": "${input:sfdxProjectDirectoryPath}",
		"args": [
		  "sgd:source:delta",
		  "-f",
		  "${input:sgdSourceDeltaFrom}",
		  "-W",
		]
	  },
	  {
		"type": "node",
		"request": "launch",
		"name": "simple: sgd:source:delta -d -W",
		"program": "${workspaceFolder}/bin/run",
		"cwd": "${input:sfdxProjectDirectoryPath}",
		"args": [
		  "sgd:source:delta",
		  "-f",
		  "${input:sgdSourceDeltaFrom}",
		  "-d",
		  "-W"
		]
	  },
	],
	"inputs": [
		{
			"id": "sfdxProjectDirectoryPath",
			"type": "promptString",
			"description": "Full path of a sfdx project to run ocliff command on"
		},
		{
			"id": "sgdSourceDeltaTo",
			"type": "promptString",
			"description": "commit sha to where the diff is done",
			"default": "HEAD"
		},
		{
			"id": "sgdSourceDeltaFrom",
			"type": "promptString",
			"description": "commit sha from where the diff is done [git rev-list --max-parents=0 HEAD]"
		},
		{
			"id": "sgdSourceDeltaRepo",
			"type": "promptString",
			"description": "git repository location",
			"default": "."
		},
		{
			"id": "sgdSourceDeltaIgnore",
			"type": "promptString",
			"description": "file listing paths to explicitly ignore for any diff actions"
		},
		{
			"id": "sgdSourceDeltaIgnoreDestructive",
			"type": "promptString",
			"description": "file listing paths to explicitly ignore for any destructive actions"
		},
		{
			"id": "sgdSourceDeltaSource",
			"type": "promptString",
			"description": "source folder focus location related to --repo",
			"default": "."
		},
		{
			"id": "sgdSourceDeltaOutput",
			"type": "promptString",
			"description": "source package specific output",
			"default": "./output"
		},
		{
			"id": "sgdSourceDeltaApiVersion",
			"type": "promptString",
			"description": "salesforce metadata API version, default to sfdx-project.json \"sourceApiVersion\" attribute or latest version"
		},
		{
			"id": "sgdSourceDeltaInclude",
			"type": "promptString",
			"description": "file listing paths to explicitly include for any diff actions"
		},
		{
			"id": "sgdSourceDeltaIncludeDestructive",
			"type": "promptString",
			"description": "file listing paths to explicitly include for any destructive actions"
		},
	]
  }